@prefix nss: <http://example.org/nss/> .
@prefix u: <http://example.org/resource/user/> .
@prefix g: <http://example.org/resource/group/> .

@prefix ex: <http://example.org/> .

u:Yoda
  a nss:User ;
  nss:name "yoda" ;
  nss:passwd "foobar" ;
  nss:uid 10000 ;
  nss:gid 10000 ;
  nss:gecos: "foobar" ;
  nss:dir "/home/yoda" ;
  nss:shell "/bin/bash" ;
  ex:server "qrowd" ;
  .

g:Yoda
  a nss:Group ;
  nss:name "yoda" ;
  nss:passwd "yoda" ;
  nss:gid 10000 ;
  nss:member "yoda", "test" ;
  ex:server "qrowd" ;
  .
  
  
